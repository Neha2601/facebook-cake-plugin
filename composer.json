{
    "name": "neha2601/facebook-cake-component",
    "description": "Facebook plugin for CakePHP",
    "license": "MIT",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "facebook", "api"],
    "homepage": "https://github.com/Neha2601/facebook-cake-component",
    "authors": [
        {
            "name": "Neha Dobariya",
            "homepage": "https://github.com/Neha2601/",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.1.0 <4.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-master",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "FacebookApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
